# Allow changing of the Prompt?
# It just clears precmd functions before running my own
precmd_functions=""

# Fix Home, End, and Delete keys
bindkey  "^[[H"   beginning-of-line
bindkey  "^[[F"   end-of-line
bindkey  "^[[3~"  delete-char

# Set the global aliases
source ~/.aliases

# Use "Oh My ZSH!"
#source ~/.zshrc.oh-my-zsh

# Use the CThemes program
source cthemes set-theme zsh doubleLined
